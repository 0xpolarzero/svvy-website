---
import { cn } from '@/lib/utils'

const { class: className = '', ...rest } = Astro.props
const classes = cn('w-3 h-3 sm:w-3.5 sm:h-3.5', className)
---

<svg
  xmlns='http://www.w3.org/2000/svg'
  class={classes}
  viewBox='0 0 32 32'
  fill='none'
  aria-hidden='true'
  preserveAspectRatio='xMidYMid'
  {...rest}
>
  <!-- Monochrome wordmark that follows currentColor -->
  <path fill='currentColor' d='M5.364 26.636h10.543V10.728h5.364v15.908h5.364V5.364H5.364z'></path>
</svg>
